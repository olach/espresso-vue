<?xml version="1.0" encoding="UTF-8"?>
<!--

CodeSense Libraries simply define lists of completions, categorized by keys. They don't define any criteria for when they should be used; this is the domain of a CodeSense Provider.

-->
<codesense version="1.0">
	
	<!-- Element-specific attribute completions -->
	
	<!--<template lang="...">-->
	<set name="com.github.olach.vue-sfc.tag.template.attribute-values.lang">
		<completion string="html" />
	</set>
	
	<!--<style lang="...">-->
	<set name="com.github.olach.vue-sfc.tag.style.attribute-values.lang">
		<completion string="css" />
		<completion string="scss" />
		<completion string="less" />
		<completion string="postcss" />
	</set>
	
	<!--<script lang="...">-->
	<set name="com.github.olach.vue-sfc.tag.script.attribute-values.lang">
		<completion string="js" />
		<completion string="coffee" />
	</set>
	
</codesense>
